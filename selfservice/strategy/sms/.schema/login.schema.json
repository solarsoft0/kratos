{
  "$id": "https://schemas.ory.sh/kratos/selfservice/strategy/sms/login.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "phone",
    "method"
  ],
  "properties": {
    "csrf_token": {
      "type": "string"
    },
    "phone": {
      "type": "string",
      "minLength": 1
    },
    "code": {
      "type": "string",
      "minLength": 1
    },
    "method": {
      "type": "string"
    }
  }
}
